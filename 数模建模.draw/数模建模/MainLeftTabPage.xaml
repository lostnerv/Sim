<UserControl x:Class="数模建模.MainLeftTabPage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <TabControl Grid.Row="1" DockPanel.Dock="Left">
        <TabItem Header="井号筛选" Name="tabItem2">
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto"/>
                    <RowDefinition Height="*"/>
                    <RowDefinition Height="Auto"/>
                </Grid.RowDefinitions>
                <ComboBox x:Name="WellBlock" Grid.Row="0" Initialized="WellBlock_Initialized"></ComboBox>
                <ListBox x:Name="wellList" Initialized="wellList_Initialized" Grid.Row="1"></ListBox>
            </Grid>
        </TabItem>
    </TabControl>
</UserControl>
